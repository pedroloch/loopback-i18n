{
  "name": "@pedroloch/loopback-i18n",
  "description": "Internationalization for LoopBack 4",
  "version": "0.0.1",
  "keywords": [
    "LoopBack",
    "i18n"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "author": "Pedro Loch",
  "engines": {
    "node": "16 || 17 || 18"
  },
  "scripts": {
    "build": "lb-tsc",
    "clean": "lb-clean loopback-apiconnect*.tgz dist *.tsbuildinfo package",
    "pretest": "npm run build",
    "test": "lb-mocha --allow-console-logs \"dist/__tests__\""
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "README.md",
    "dist",
    "src",
    "!*/__tests__"
  ],
  "peerDependencies": {
    "@loopback/core": "^3.1.2",
    "@loopback/rest": "^12.0.3"
  },
  "dependencies": {
    "i18n": "^0.15.1",
    "tslib": "^2.0.0"
  },
  "devDependencies": {
    "@loopback/build": "^8.1.2",
    "@loopback/eslint-config": "^12.0.4",
    "@loopback/rest": "^11.1.2",
    "@loopback/testlab": "^4.1.2",
    "@types/i18n": "^0.13.4",
    "@types/node": "^12.20.47"
  }
}
